{
  "name": "bedrock-claude-multimodal",
  "displayName": "Amazon Bedrock - Claude MultiModal",
  "version": "0.0.1",
  "scope": "public",
  "description": "Amazon Bedrock - Claude Multimodal Models Application. This application enables access to AWS clients, allowing you to retrieve responses from the Claude multimodal models. Utilize your API keys to interact seamlessly with advanced AI capabilities that process and analyze both text and visual inputs.",
  "attributes": {
    "Provider": "Anthropic",
    "Deployed By": "AWS",
    "Category": "Model",
    "NLP": "Conversational",
    "Gen AI": "LMM",
    "Media Type": [
      "Text",
      "Image",
      "Multi Modal"
    ]
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/amazon-bedrock.git",
    "gitTag": "0.0.1"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "claude-multimodal-service",
        "runtime": {
          "podType": "regular-s",
          "concurrency": 1,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 1,
            "queueLength": 100
          }
        }
      }
    ],
    "modules": [
      {
        "name": "claude-multimodal-module",
        "entryPoint": "models/claude/claude_multimodal.py",
        "className": "ModelAdapter",
        "computeConfig": "claude-multimodal-service",
        "description": "Claude MultiModal Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate model performance"
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Function to run inference on items"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": ""
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
            ],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Function to run inference on a dataset"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "anthropic-claude-sonnet-3",
        "moduleName": "claude-multimodal-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "system_prompt": "You are a helpful and a bit cynical assistant. Give relevant and short answers, if you dont know the answer just say it, dont make up an answer",
          "model_id": "anthropic.claude-3-sonnet-20240229-v1:0",
          "region": "",
          "max_tokens": 200
        },
        "description": "Bedrock call for Claude 3 Sonnet."
      },
      {
        "name": "anthropic-claude-sonnet-3-5",
        "moduleName": "claude-multimodal-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "system_prompt": "You are a helpful and a bit cynical assistant. Give relevant and short answers, if you dont know the answer just say it, dont make up an answer",
          "model_id": "anthropic.claude-3-5-sonnet-20240620-v1:0",
          "region": "",
          "max_tokens": 200
        },
        "description": "Bedrock call for Claude 3.5 Sonnet."
      },
      {
        "name": "anthropic-claude-instant-v1",
        "moduleName": "claude-multimodal-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "system_prompt": "You are a helpful and a bit cynical assistant. Give relevant and short answers, if you dont know the answer just say it, dont make up an answer",
          "model_id": "anthropic.claude-3-haiku-20240307-v1:0",
          "region": "",
          "max_tokens": 200
        },
        "description": "Bedrock call for Claude 3 Haiku."
      }
    ]
  }
}